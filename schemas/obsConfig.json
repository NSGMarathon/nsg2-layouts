{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "videoInputsScene": { "type": "string" },
        "gameLayoutVideoFeedScenes": {
            "type": "array",
            "items": { "type": ["string", "null"] }
        },
        "gameplayScenes": {
            "type": "array",
            "items": { "type": ["string", "null"] }
        },
        "intermissionScene": { "type": "string" },
        "interstitialVideoScene": { "type": "string" }
    },
    "required": ["gameLayoutVideoFeedScenes", "gameplayScenes"],
    "default": {
        "gameLayoutVideoFeedScenes": [null, null, null],
        "gameplayScenes": [null, null, null]
    }
}
